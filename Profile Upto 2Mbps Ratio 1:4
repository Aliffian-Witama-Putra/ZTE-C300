#Registrasi Onu dengan bandwith Ratio up to dw 1:4
#Profile TCON 4 (BEST-EFFORT CD 'UPBE')
#Profile Bandwith 2Mbps

interface gpon-onu_INTERFACE_GPON_ONU
  name ID_PELANGGAN
  description DESCRIPTION_PELLANGGAN
  sn-bind enable sn
  tcont 1 name HSI profile UPBE2M
  gemport 1 name HSI unicast tcont 1 dir both
  gemport 1 traffic-limit upstream UP500K downstream DW2M
  switchport mode hybrid vport 1
  service-port 1 vport 1 user-vlan 1200 vlan 1200
  pppoe-plus enable sport 1
  pppoe-plus trust true replace sport 1
exit
